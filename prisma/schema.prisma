generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")

}

model Game {
  id        String   @id @default(cuid())
  p1        String
  p2        String
  p3        String
  p4        String
}

model Events {
  id         String   @id @default(cuid())
  timestamp  DateTime @default(now())
  gameId     String
  type       String
  playerId     String
  value     String
}